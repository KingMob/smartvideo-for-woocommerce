{
	"name": "extension/smartvideo-for-woocommerce",
	"description": "SmartVideo for WooCommerce plugin",
	"autoload": {
		"psr-4": {
			"SmartvideoForWoocommerce\\": "includes/"
		}
	},
	"require-dev": {
		"automattic/jetpack-autoloader": "^2",
		"squizlabs/php_codesniffer": "^3.7",
		"woocommerce/woocommerce-sniffs": "^0.1.3",
		"woocommerce/qit-cli": "^0.3.4"
	},
	"config": {
		"allow-plugins": {
			"automattic/jetpack-autoloader": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
